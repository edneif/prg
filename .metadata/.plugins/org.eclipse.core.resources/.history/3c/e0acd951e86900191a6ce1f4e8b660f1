#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int a,b,c,r,rant,rpro;

	a= 0x100;  //end
	b= 0x542;  //xor 442
	c= 0x300;  //xor 742
	d =0x200;  //xor


	r = a ^ b;

   rant = r ^ a;



 printf ( "   %x\n   %x\n = %x\n   %x\n", a,b,r,rant);

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
