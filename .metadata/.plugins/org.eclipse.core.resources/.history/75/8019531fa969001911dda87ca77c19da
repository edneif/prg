#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int a,b,r,rant,rpro;

	a= 0x100;
	b= 0x542;

	r = a ^ b;

   rant = a ^ r;



 printf ( "   %x\n   %x\n = %x\n   %x\n", a,b,r,rant);

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
